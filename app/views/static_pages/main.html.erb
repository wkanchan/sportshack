
<div classs="row">
  <%= render "layouts/sidebar"%><!-- size 2 -->
  
  <div class="col-md-10">
    <div class="row">
      <img src="http://www.development.squadcloud.com/images/controlpanelheader.png" class="img-rounded">
      <img src="http://www.development.squadcloud.com/images/squadcloud.png" alt="SQ" class="img-rounded">

    </div>

    <div class="row">
      <%= image_tag 'clock.png', :alt => 'clock' %>
    </div>

    <div class="row"  style="margin-top: 20px;">
      <div class="col-md-2">
      
        <button type="button" class="btn btn-default btn-lg">
          <span class="glyphicon glyphicon-user"></span> <div>User<div>
        </button> 
      </div>

      <div class="col-md-2">
      
        <button type="button" class="btn btn-default btn-lg">
          <span class="glyphicon glyphicon-user"></span> <div>Friends<div>
        </button> 
      </div>

      <div class="col-md-2">
      
        <button type="button" class="btn btn-default btn-lg">
          <span class="glyphicon glyphicon-user"></span> <div>Team<div>
        </button> 
      </div>

      <div class="col-md-2">
      
        <button type="button" class="btn btn-default btn-lg">
          <span class="glyphicon glyphicon-user"></span> <div>Games<div>
        </button> 
      </div>

    </div>


    <div class="row" style="margin-top: 20px;">
      <div class="col-md-2">
      
        <button type="button" class="btn btn-default btn-lg">
          <span class="glyphicon glyphicon-user"></span> <div>Scout<div>
        </button> 
      </div>

      <div class="col-md-2">
      
        <button type="button" class="btn btn-default btn-lg">
          <span class="glyphicon glyphicon-user"></span> <div>Presss<div>
        </button> 
      </div>

      <div class="col-md-2">
      
        <button type="button" class="btn btn-default btn-lg">
          <span class="glyphicon glyphicon-user"></span> <div>Leagues<div>
        </button> 
      </div>

      <div class="col-md-2">
      
        <button type="button" class="btn btn-default btn-lg">
          <span class="glyphicon glyphicon-user"></span> <div>Tournaments<div>
        </button> 
      </div>

    </div>


  </div>

</div>  

